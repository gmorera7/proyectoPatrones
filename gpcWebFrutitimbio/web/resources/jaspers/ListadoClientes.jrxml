	<queryString>
		<![CDATA[SELECT
     cliente.`nit`,
     cliente.`nombre`,
     cliente.`representante_legal`,
     d.descripcion,
     m.nombreMunicipio,
     cliente.`direccion`,
     cliente.`tipo_telefono`,
     cliente.`numero_telefono`,
     cliente.`email`,
     cliente.`estado`
FROM
     `cliente` cliente
	  inner join municipio m
	  inner join departamentos d
where  m.idmunicipio  = `cliente`.`municipio_idmunicipio`
      and m.departamento_iddepartamento = d.iddepartamento


 and   `cliente`.`nit` = IFNULL($P{nit},CLIENTE.NIT)]]>
	</queryString>

		</parameter>
		<queryString>
			<![CDATA[SELECT c.codigo_compra, 
       c.fecha_compra, 
       c.total,
       p.numero_identificacion, 
       p.nombre1, 
       p.apellido1
FROM compra c
     INNER JOIN proveedor p ON c.proveedor_codigo = p.codigo

WHERE c.fecha_compra BETWEEN  $P{fecha_inicio} AND  $P{fecha_fin}
LIMIT 0 , 30]]>
		</queryString>
